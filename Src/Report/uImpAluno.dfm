  object qAluno: TZQuery
    Connection = DmCliente.conBD
    SQL.Strings = (
      'select'
      '  id, idEscola, nome, dtNasc,'
      '  NomePai, NomeMae, nomePrograma,'
      '  cargaHoraria, periodo, infoSaude, infoEspecial,'
      '  case ativo when 1'
      '    then '#39'S'#39
      '    else '#39'N'#39' end ativo'
      'from aluno'
      'where deleted is null'
      '  and (((0 = :id) or (id = :id)))'
      '  and nome like :pesq;')
    Params = <
